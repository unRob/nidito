<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>io.vault.daemon</string>
    <key>UserName</key>
    <string>{{ vault.user }}</string>
    <key>Disabled</key>
    <false/>
    <key>RunAtLoad</key>
    <true/>
    <key>KeepAlive</key>
    <true/>
    <key>StandardOutPath</key>
    <string>{{ vault.logs }}/out.log</string>
    <key>StandardErrorPath</key>
    <string>{{ vault.logs }}/error.log</string>
    <key>ProgramArguments</key>
    <array>
        <string>{{ vault.bin }}</string>
        <string>server</string>
        <string>-config</string>
        <string>{{ vault.config }}/config.hcl</string>
    </array>
</dict>
</plist>
